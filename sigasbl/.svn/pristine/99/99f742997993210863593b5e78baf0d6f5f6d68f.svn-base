package it.csi.sigas.sigasbl.model.vo.impostazioni;

import it.csi.sigas.sigasbl.common.rest.VO;

public class TipoAliquoteVO implements VO {

	private static final long serialVersionUID = -1L;

	private Long idTipoAliquota;
	private String descrizione;
	private String nomeAliquota;
	private boolean nuovoAllacciamento;
	private String tipo;
	private TipoConsumoVO tipoConsumo;
	
	public Long getIdTipoAliquota() {
		return idTipoAliquota;
	}
	public void setIdTipoAliquota(Long idTipoAliquota) {
		this.idTipoAliquota = idTipoAliquota;
	}
	public String getDescrizione() {
		return descrizione;
	}
	public void setDescrizione(String descrizione) {
		this.descrizione = descrizione;
	}
	public String getNomeAliquota() {
		return nomeAliquota;
	}
	public void setNomeAliquota(String nomeAliquota) {
		this.nomeAliquota = nomeAliquota;
	}
	public boolean isNuovoAllacciamento() {
		return nuovoAllacciamento;
	}
	public void setNuovoAllacciamento(boolean nuovoAllacciamento) {
		this.nuovoAllacciamento = nuovoAllacciamento;
	}
	public String getTipo() {
		return tipo;
	}
	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
	public TipoConsumoVO getTipoConsumo() {
		return tipoConsumo;
	}
	public void setTipoConsumo(TipoConsumoVO tipoConsumo) {
		this.tipoConsumo = tipoConsumo;
	}
}
